/* components/nav/nav.wxss */
@import "./wxss/main.wxss";
.nav-main{
  position:fixed;
  width: 100%;
  z-index: 1;
}
.nav{
  font-size: 30rpx;
  font-weight: bold;
}
.nav .part{
  text-align: center;

}
.nav .part .title{
  width: 100%;
  overflow: hidden;
  white-space:nowrap;
}
.nav .part .opt{
  min-width: 188rpx;
  height: 100%;
}
.nav .part .opt view{
  width: 68rpx;
  height: 100%;
  margin-left: 26rpx;
}
.nav .part .opt .back-image{
  height: 32rpx;
  width: 18rpx;
}
.nav .part .opt .home-image{
  height: 34rpx;
  width: 36rpx;
}
